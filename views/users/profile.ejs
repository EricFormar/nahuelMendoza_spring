
    <%-include("../partials/headerAdm") %>
        <body>
            
        
    <main class="table-admin">

            <div class="titulo-admin">
                <h1 class="text-center">Perfil de usuario</h1>
            </div>
            <div class="class-table">
                <table>
                    <thead>
                        
                        <tr></tr>
                            <th>#</th>    
                            <th>Producto</th>
                            <th>Precio</th>
                            <th>Piezas</th>
                            <th>Categoria</th>
                            <th>acciones</th>
                                                    
                    </thead>
                    <tbody>
                        <% product.forEach(({id,nombre,precio,piezas,categoria})=> { %>

                            <tr>
                                <td>
                                <%=id %>
                            </td>
                            <td>
                                <%= nombre %>
                            </td>
                            <td>
                                $<%= precio %>
                            </td>
                            <td>
                                <%= piezas %> ud.
                            </td>
                            <td>
                                <%= categoria %>
                            </td>
                            <td>
                                <div>
                                    <a href="" class="btn-table">editar</a>
                                </div>
                            </td>
                            <td>
                                <div>
                                    <a href="" class="btn-eliminar">eliminar</a>
                                </div>
                            </td>
                        </tr>
                            <% }) %>
                    </tbody>
                </table>
            </div>
        </main>
    </body>